{
  "dependencies": {
    "discord-protos": "^1.0.5",
    "electron-store": "^8.1.0",
    "genius-lyrics": "^4.4.7",
    "lyrics-finder": "^21.7.0"
  },
  "name": "lyriccord",
  "version": "1.2.0",
  "main": "dist/main.js",
  "devDependencies": {
    "@types/electron": "^1.4.38",
    "@types/electron-store": "^1.3.1",
    "@types/node": "^22.5.0",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "start": "tsc && electron .",
    "build": "tsc",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --win portable"
  },
  "build": {
    "appId": "com.lyriccord.app",
    "productName": "LyricCord",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "index.html",
      "config.html",
      "icons/*"
    ],
    "win": {
      "target": "portable",
      "icon": "icons/icon.ico",
      "sign": null,
      "signDlls": false,
      "signAndEditExecutable": false
    },
    "asar": false,
    "forceCodeSigning": false,
    "npmRebuild": false
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "Discord status updater with song lyrics"
}
